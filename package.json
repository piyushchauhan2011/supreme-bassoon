    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "jest": "^27.2.5",
    "jest-haste-map": "^27.2.5",
    "jest-resolve": "^27.2.5",
    "jest-styled-components": "^7.0.5",
    "lerna": "^4.0.0",
